<table class="table table-hover">
  <thead>
    <tr>
      <th>#</th>
      <th>Nome</th>
      <th>Valor</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% currencies.each do |currency| %>
      <tr>
        <td><%= currency.id %></td>
        <td><%= currency.name %></td>
        <td>$ <%= number_with_precision(currency.price, precision: 5) %></td>
        <td>
          <%= link_to '#', class:"btn btn-primary btn-circle", disabled: "disabled"  do %>
              <i class="fa fa-eye"></i>
          <% end %>

          <%= link_to '#',class:"btn btn-danger btn-circle", disabled: "disabled" do %>
            <i class="fa fa-minus"></i>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>